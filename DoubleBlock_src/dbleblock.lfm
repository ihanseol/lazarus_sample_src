    Left = 12
    Height = 150
    Top = 48
    Width = 1100
    Color = clCream
    Font.Height = -17
    Lines.Strings = (
      'Double Block rules:'
      ''
      '1. Color exactly 2 fields in each row and each column of the NxN grid.'
      '2. In each of the resting fields, write a number between 1 and N-2.'
      '3. In each row and each column, each number must be written exactly once.'
      '4. The numbers at the left and the top indicate the sum of the numbers between the 2 colored fields in the corresponding row resp. column.'
    )
    ParentFont = False
    ReadOnly = True
    TabOrder = 2
